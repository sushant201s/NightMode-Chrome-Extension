{
    "manifest_version": 2,
    "name": "Night Mode",
    "version": "1.0",
    "description": "This will darken your web page to continue browsing at night without stressing your eyes",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    }
}